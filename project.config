[access]
	inheritFrom = All-Projects
[submit]
	action = inherit
[access "refs/*"]
	owner = group github/eclipse-egit/eclipsefdn-releng
	owner = group github/eclipse-egit/eclipsefdn-security
	owner = group user/Matthias Sohn (msohn)
	read = group Anonymous Users
	revert = group Registered Users
	forgeCommitter = block group Anonymous Users
	forgeServerAsCommitter = block group Anonymous Users
[access "refs/for/refs/*"]
	push = group Registered Users
	pushMerge = group Registered Users
[access "refs/meta/config"]
	create = group Project Owners
	label-Code-Review = -2..+2 group Project Owners
	label-Verified = -1..+1 group Project Owners
	read = group Project Owners
	read = group github/eclipse-egit/technology-egit-committers
	read = block group Anonymous Users
	submit = group Project Owners
	push = group Project Owners
	abandon = group Project Owners
	exclusiveGroupPermissions = abandon create label-Code-Review label-Verified push submit
[access "refs/for/*"]
	addPatchSet = group Registered Users
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group github/eclipse-egit/technology-egit-committers
	label-Code-Review = -1..+1 group Registered Users
	label-Verified = -1..+1 group github/eclipse-egit/technology-egit-committers
	label-Verified = -1..+1 group user/eclipse-egit-bot (eclipse-egit-bot)
	abandon = group github/eclipse-egit/technology-egit-committers
	create = group github/eclipse-egit/technology-egit-committers
	forgeAuthor = group github/eclipse-egit/technology-egit-committers
	submit = group github/eclipse-egit/technology-egit-committers
	exclusiveGroupPermissions = abandon create forgeAuthor label-Code-Review label-Verified submit
[access "refs/tags/*"]
	create = group github/eclipse-egit/technology-egit-committers
	createSignedTag = group github/eclipse-egit/technology-egit-committers
	createTag = group github/eclipse-egit/technology-egit-committers
	push = +force group github/eclipse-egit/technology-egit-committers
	exclusiveGroupPermissions = create createSignedTag createTag

[trackingid "bugfooter"]
	footer = Bug
	match = (egit-\\d+)
	system = GitHub
[receive]
	requireChangeId = true
[plugin "zuul-results-summary"]
	enabled = false
[plugin "eca-validation"]
	enabled = true
[plugin "uploadvalidator"]
	blockedContentTypeWhitelist = false
	maxPathLength = 0
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = false
	rejectSymlink = false
	rejectWindowsLineEndings = false
[commentlink "buglink"]
	match = ([Bb][Uu][Gg]:\\s+)(egit-)(\\d+)(\\s*)
	link = https://github.com/eclipse-egit/egit/issues/$3
	prefix = $1
	suffix = $4
	text = $2$3
[commentlink "changeid"]
	match = (.*\\b)(I[0-9a-f]{7,40})(\\b.*)
	link = /q/$2
	prefix = $1
	suffix = $3
	text = $2
